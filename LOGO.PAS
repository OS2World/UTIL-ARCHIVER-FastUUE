procedure ShowLogo;
 var
  K: Integer;
  B, C: Byte;
  S: String;
 begin
  vSetAttr($07);
  vClrScr;

  B:=vGetAttr;

  S:='';

  for K:=1 to 22 * 80 do
   begin
    C:=Byte(Logo1[K * 2]);

    if C <> B then
     begin
      vPrint(S);
      S:='';

      B:=C;
      vSetAttr(C);
     end;

    S:=Concat(S, Logo1[K * 2 - 1]);
   end;

  vPrint(S);

  vSetAttr($07);
 end;

end.
