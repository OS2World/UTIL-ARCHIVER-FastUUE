scr.uue.trying.section.error="$0csection error%title%: %hilight%%1%title%/%hilight%%2%title%/%hilight%%3%title%/%hilight%%4%title%/%hilight%%5%title%/%hilight%%6%title%/%hilight%%7%title%/%hilight%%8%title%/%hilight%%9%title%#0d#0a"
log.uue.trying.section.error="Section error %1/%2/%3/%4/%5/%6/%7/%8/%9, resource file may be corrupted."
