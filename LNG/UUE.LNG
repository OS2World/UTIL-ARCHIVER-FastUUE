log.uue.trying.section.crc="File %1 CRC error: c %2h/f %3h."
;
scr.uue.trying.done="%normal%ok#0d#0a"
log.uue.trying.done="%1 ok."
;
scr.uue.trying.group="%warning%%uuehead% Can't find group for area %title%%1%normal%.#0d#0a"
log.uue.trying.group="Cannot find group for area %1."
;
