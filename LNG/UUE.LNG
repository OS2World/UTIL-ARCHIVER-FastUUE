;
scr.uue.we.must.kill.this="%info%%uuehead%  $08*%normal% dupe, killed.#0d#0a"
log.uue.we.must.kill.this="Dupe, killed."
;
scr.uue.incomplete.file="%info%%uuehead% Incomplete file: %hilight%%1%normal%: %hilight%%2%normal% sections of %hilight%%3%normal%, days %hilight%%4%normal% of %hilight%%5%normal%.#0d#0a"
log.uue.incomplete.file="Incomplete %1: %2 sections of %3."
;
scr.uue.complete.file="%info%%uuehead% Complete file: %hilight%%1%normal%: %hilight%%2%normal% sections of %hilight%%3%normal%, days %hilight%%4%normal% of %hilight%%5%normal%.#0d#0a"
log.uue.complete.file="Complete %1: %2 sections of %3."
;
